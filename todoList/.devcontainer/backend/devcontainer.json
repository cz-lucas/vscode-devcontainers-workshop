{
    "name": "Backend",
    "service": "backend",
    "dockerComposeFile": [
        "../../docker-compose.yaml"
    ],
    "forwardPorts": [
        8000
    ],
    "postCreateCommand": "cd backend ; python -m pip install -r requirements.txt",
    "shutdownAction": "none",
    "workspaceMount": "source=${localWorkspaceFolder],target=/workspace,type=bind",
    "workspaceFolder": "/workspace"
}
